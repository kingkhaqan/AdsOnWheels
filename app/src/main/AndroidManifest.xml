<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zeesh.aowprototype">

    <permission
        android:name="com.example.mapexdemo.permission.MAPS_RECEIVE"
        android:protectionLevel="signature" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity
            android:name=".LoginActivity"
            android:theme="@style/LoginTheme" />
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_dashboard" />
        <activity
            android:name=".EstimationActivity"
            android:label="@string/title_activity_estimation" />
        <activity
            android:name=".SignupActivity"
            android:theme="@style/LoginTheme" />
        <activity
            android:name=".DriverListActivity"
            android:label="@string/title_activity_driverlist" />
        <activity
            android:name=".ProfileActivity"
            android:label="@string/title_activity_profile" />
        <activity
            android:name=".DashboardActivity"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DriverHomeActivity"
            android:label="@string/title_activity_driver_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SplashActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".DriverLocationActivity" />
        <activity android:name=".NotificationActivity" />
        <activity android:name=".UserProfileActivity" />
        <activity
            android:name=".AdminDashboardActivity"
            android:label="@string/title_activity_admin_dashboard"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".UserDetailActivity"
            android:label="@string/title_activity_user_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <!-- <activity -->
        <!-- android:name=".CampeignNotificationActivity" -->
        <!-- android:label="@string/title_activity_campeign_notification"></activity> -->
        <activity android:name=".ConfirmCompaignActivity" />
        <activity android:name=".PendingCampaignActivity" />
        <activity android:name=".CampaignListActivity"></activity>
    </application>

</manifest>